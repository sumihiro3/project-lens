{
  "name": "project-lens",
  "version": "0.2.0",
  "description": "ProjectLens",
  "author": "Sumihiro Kagawa",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "tauri:dev": "npm run generate && cargo run --manifest-path=src-tauri/Cargo.toml",
    "tauri:build": "npm run generate && npx tauri build && node scripts/organize-dist.js",
    "build:release": "./build.sh",
    "lint": "eslint --ext .ts,.vue,.js .",
    "lint:fix": "eslint --ext .ts,.vue,.js . --fix",
    "lint:rust": "cargo clippy --manifest-path=src-tauri/Cargo.toml -- -D warnings",
    "format": "prettier --write \"**/*.{ts,vue,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,vue,js,json,md}\"",
    "format:rust": "cargo fmt --manifest-path=src-tauri/Cargo.toml",
    "format:rust:check": "cargo fmt --manifest-path=src-tauri/Cargo.toml -- --check",
    "version-major": "npm version major",
    "version-premajor": "npm version premajor --preid=beta",
    "version-minor": "npm version minor",
    "version-preminor": "npm version preminor --preid=beta",
    "version-patch": "npm version patch",
    "version-prepatch": "npm version prepatch --preid=beta",
    "version-prerelease": "npm version prerelease --preid=beta",
    "version": "node scripts/sync-version.js",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@tauri-apps/plugin-http": "^2.5.4",
    "@tauri-apps/plugin-log": "^2.7.1",
    "@tauri-apps/plugin-notification": "^2.3.3",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "@tauri-apps/plugin-sql": "^2.3.1",
    "@tauri-apps/plugin-window-state": "^2.4.1",
    "chart.js": "^4.5.1",
    "nuxt": "^4.2.1",
    "vite-plugin-vuetify": "^2.1.2",
    "vue": "^3.5.24",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.3",
    "vuetify": "^3.10.11"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.11.0",
    "@nuxtjs/i18n": "^10.2.1",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/cli": "^2.9.4",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.2"
  }
}