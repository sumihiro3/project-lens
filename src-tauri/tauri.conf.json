{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "ProjectLens",
  "version": "0.2.0",
  "identifier": "com.tep-lab.project-lens",
  "build": {
    "frontendDist": "../.output/public"
  },
  "app": {
    "windows": [
      {
        "title": "ProjectLens",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "icons/TrayIconTemplate.png"
    ],
    "macOS": {
      "minimumSystemVersion": "10.13"
    }
  }
}
